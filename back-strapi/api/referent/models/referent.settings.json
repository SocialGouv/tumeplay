{
  "kind": "collectionType",
  "collectionName": "referents",
  "info": {
    "name": "Lieux de rencontres",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "environnement": {
      "model": "environnement"
    },
    "address": {
      "type": "string"
    },
    "address_zipcode": {
      "type": "string"
    },
    "address_city": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "openingHours": {
      "type": "component",
      "repeatable": false,
      "component": "referents.opening-hours"
    },
    "description": {
      "type": "text"
    },
    "is_available": {
      "type": "boolean",
      "default": true
    }
  }
}
